    {
      "parameters": {
        "jsCode": "let classes = [];\nlet assignments = [];\nlet exams = [];\nlet warnings = [];\n\nitems.forEach(item => {\n  if (item.json.Start_Time) {\n    classes.push(`‚Ä¢ ${item.json.Subject} (${item.json.Start_Time}‚Äì${item.json.End_Time})`);\n  }\n\n  if (item.json.Task_Name) {\n    assignments.push(`‚Ä¢ ${item.json.Task_Name} (${item.json.Days_Left} days left)`);\n  }\n\n  if (item.json.Exam_Date) {\n    exams.push(`‚Ä¢ ${item.json.Subject} (${item.json.Days_Left} days left)`);\n  }\n\n  if (item.json.Attendance_Percentage) {\n    warnings.push(`‚Ä¢ ${item.json.Subject} attendance below required`);\n  }\n});\n\nreturn [{\n  json: {\n    email_body: `\nüéì College Daily Briefing\n\nüìö Classes Today:\n${classes.length ? classes.join(\"\\n\") : \"‚Ä¢ None\"}\n\nüìù Urgent Assignments:\n${assignments.length ? assignments.join(\"\\n\") : \"‚Ä¢ None\"}\n\nüìñ Upcoming Exams:\n${exams.length ? exams.join(\"\\n\") : \"‚Ä¢ None\"}\n\n‚ö†Ô∏è Warnings:\n${warnings.length ? warnings.join(\"\\n\") : \"‚Ä¢ None\"}\n`\n  }\n}];\n"
      },
